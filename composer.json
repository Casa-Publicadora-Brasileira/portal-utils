{
    "name": "casa-publicadora-brasileira/portal-utils",
    "type": "library",
    "description": "Ferramentas genéricas do Portal da Educação da CPB",
    "keywords": [
        "library",
        "utils",
        "Portal da educação",
        "CPB"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Casa-Publicadora-Brasileira/portal-utils",
    "repositories": [
        {
            "type": "composer",
            "url": "https://github.com/Casa-Publicadora-Brasileira/portal-utils",
            "canonical": false
        }
    ],
    "autoload": {
        "psr-4": {
            "CasaPublicadoraBrasileira\\PortalUtils\\": "src/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "sentry/sentry-laravel": "^4.13",
        "laravel/framework": "^8.75",
        "tymon/jwt-auth": "^1.0",
        "aws/aws-sdk-php": "*"
    }
}
